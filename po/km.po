#: plugins/Unity/Launcher/launcheritem.cpp:43
#: plugins/Unity/Launcher/launcheritem.cpp:73
msgid "Lock to Launcher"
msgstr "ចាក់​សោ​​ទៅ​កម្មវិធី​ចាប់ផ្ដើម"

#: plugins/Unity/Launcher/launcheritem.cpp:73
msgid "Unlock from Launcher"
msgstr "ដោះសោ​ពី​កម្មវិធី​ចាប់ផ្ដើម"

#: qml/Components/EdgeDemoOverlay.qml:151
msgid "Skip intro"
msgstr "រំលង​ការ​ណែនាំ"

#: qml/Components/EdgeDemo.qml:113
msgid "Right edge"
msgstr "គែម​ស្ដាំ"

#: qml/Components/EdgeDemo.qml:114
msgid "Try swiping from the right edge to unlock the phone"
msgstr "សាកល្បង​​អូស​ពី​គែម​ស្ដាំ​ ដើម្បី​ដោះ​សោ​ទូរស័ព្ទ"

#: qml/Components/EdgeDemo.qml:145
msgid "Top edge"
msgstr "គែម​ខាង​លើ"

#: qml/Components/EdgeDemo.qml:146
msgid "Try swiping from the top edge to access the indicators"
msgstr "សាកល្បង​អូស​ពី​គែម​ខាង​លើ ដើម្បី​ចូល​ដំណើរការ​សញ្ញា"

#: qml/Components/EdgeDemo.qml:171
msgid "Close"
msgstr "បិទ"

#: qml/Components/EdgeDemo.qml:172
msgid "Swipe up again to close the settings screen"
msgstr "អូស​ឡើង​លើ​ម្ដងទៀត ដើម្បី​បិទ​ការ​កំណត់​អេក្រង់"

#: qml/Components/EdgeDemo.qml:199
msgid "Left edge"
msgstr "គែមឆ្វេង"

#: qml/Components/EdgeDemo.qml:200
msgid "Swipe from the left to reveal the launcher for quick access to apps"
msgstr ""
"អូស​ពី​គែម​ឆ្វេង "
"ដើម្បី​បង្ហាញ​កម្មវិធី​ចាប់ផ្ដើម​សម្រាប់​ចូល​ដំណើរ​ការ​កម្មវិធី​រហ័ស"

#: qml/Components/EdgeDemo.qml:227
msgid "Well done"
msgstr "ធ្វើ​បាន​ល្អ"

#: qml/Components/EdgeDemo.qml:228
msgid ""
"You have now mastered the edge gestures and can start using the "
"phone<br><br>Tap on the screen to start"
msgstr ""
"ឥឡូវ​អ្នក​បាន​ពិនិត្យ​កាយវិការ​គែម​ "
"និង​អាច​ចាប់ផ្ដើម​ប្រើ​ទូរស័ព្ទ<br><br>ប៉ះ លើ​អេក្រង់​ដើម្បី​ចាប់ផ្ដើម"

