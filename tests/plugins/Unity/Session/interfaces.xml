    <method name="Reboot">
      <arg name="interactive" type="b" direction="in" />
    </method>
    <method name="PowerOff">
      <arg name="interactive" type="b" direction="in" />
    </method>
    <signal name="RebootCalled">
      <arg name="interactive" type="b" direction="out" />
    </signal>
    <signal name="PowerOffCalled">
      <arg name="interactive" type="b" direction="out" />
